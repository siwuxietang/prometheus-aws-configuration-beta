[
{
  "name": "auth-proxy",
  "image": "nginx:alpine",
  "cpu": 128,
  "memory": 128,
  "essential": true,
  "portMappings": [
    {
      "containerPort": 8181
    }
  ],
  "mountPoints": [
    {
      "sourceVolume": "auth-proxy",
      "containerPath": "/etc/nginx/conf.d",
      "readOnly": true
    }
  ],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": "${log_group}",
      "awslogs-region": "${region}",
      "awslogs-stream-prefix": "prometheus"
    }
  }
}
]